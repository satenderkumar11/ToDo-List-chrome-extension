{
    "name":"ToDo List",
    "version":"0.0.1",
    "manifest_version":3,


    "action":{
        "default_popup":"popup.html",
        "default_icon": {
          "16": "/images/16.png",
          "32": "/images/32.png",
          "48": "/images/48.png",
          "128": "/images/128.png"
        }


    },

    "icons": {
      "16": "/images/16.png",
      "32": "/images/32.png",
      "48": "/images/48.png",
      "128": "/images/128.png"
    },

    "permissions":["activeTabs","storage","scripting"],


    "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "MacCtrl+Shift+F"
          },
          "description": "Opens hello.html"
        }
      }
    }


